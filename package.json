{
  "name": "azula",
  "version": "1.0.0",
  "description": "Next generation music bot",
  "author": "Timic3",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "imports": {
    "#/*": "./dist/*.js",
    "#infrastructure/*": "./dist/infrastructure/*.js",
    "#services/*": "./dist/services/*.js",
    "#utils/*": "./dist/utils/*.js"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "node -r dotenv/config --preserve-symlinks --enable-source-maps dist/index.js",
    "lint": "eslint .",
    "dev": "tsup --watch --onSuccess \"npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timic3/azula.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Timic3/azula/issues"
  },
  "homepage": "https://github.com/Timic3/azula#readme",
  "engines": {
    "node": ">=v16.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@distube/ytdl-core": "^4.13.5",
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/framework": "^5.2.1",
    "@sapphire/plugin-api": "^6.1.1",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/type": "^2.4.4",
    "@sapphire/utilities": "^3.16.2",
    "discord.js": "^14.15.3",
    "play-dl": "^1.9.7",
    "prism-media": "^1.3.5",
    "sodium-native": "^4.1.1",
    "tsup": "^6.7.0",
    "undici": "^5.23.0",
    "youtube-sr": "^4.3.11"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^3.3.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.47.0",
    "ffmpeg-static": "^5.2.0",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "18.17.1"
  }
}
