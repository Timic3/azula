{
  "name": "azula",
  "version": "1.0.0",
  "description": "Next generation music bot",
  "author": "Timic3",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "imports": {
    "#/*": "./dist/*.js",
    "#infrastructure/*": "./dist/infrastructure/*.js",
    "#services/*": "./dist/services/*.js",
    "#utils/*": "./dist/utils/*.js"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "node --env-file .env --preserve-symlinks --enable-source-maps dist/index.js",
    "lint": "eslint .",
    "dev": "tsup --watch --onSuccess \"npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timic3/azula.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Timic3/azula/issues"
  },
  "homepage": "https://github.com/Timic3/azula#readme",
  "engines": {
    "node": ">=v16.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@sapphire/decorators": "^6.2.0",
    "@sapphire/framework": "^5.3.6",
    "@sapphire/plugin-api": "^8.3.1",
    "@sapphire/plugin-logger": "^4.1.0",
    "@sapphire/type": "^2.6.0",
    "@sapphire/utilities": "^3.18.2",
    "@snazzah/davey": "^0.1.6",
    "discord.js": "^14.22.1",
    "googlevideo": "^4.0.3",
    "sodium": "^3.0.2",
    "tsup": "^8.5.0",
    "undici": "^7.15.0",
    "youtubei.js": "^15.0.1"
  },
  "devDependencies": {
    "@sapphire/ts-config": "^5.0.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.57.0",
    "ffmpeg-static": "^5.2.0",
    "typescript": "^5.9.2"
  },
  "volta": {
    "node": "22.19.0"
  }
}
